    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-json": "^3.2.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "airtable": "^0.10.1",
    "chalk": "^4.1.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1",
    "unzipper": "^0.10.11",
    "winston": "^3.3.3"
